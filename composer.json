{
    "name": "josegonzalez/queue",
    "description": "Queue plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "enqueue/simple-client": "0.9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Queue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Queue\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
